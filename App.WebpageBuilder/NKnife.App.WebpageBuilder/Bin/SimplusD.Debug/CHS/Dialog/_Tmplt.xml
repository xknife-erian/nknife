<?xml version="1.0" encoding="UTF-8"?>
<resourceText>
  <!--databind节点指定数据绑定的文件源-->
  <databind level="1" id="1" file="XXXXXX.xml" />
  
  <!--form节点类似control节点-->
  <form text="频道管理" />
  
  <!--control节点用name将编程中的控件绑定起来，并做一些设置-->
  <control name="btnOK" text="确定"/>
  <control name="txtName" elementBindName="name" elementBindGroup="groupA" elementBindFormat="yyyy年M月d日 HH:ss" 
           elementBindType="CDATA"/>
  
  <!--object节点是利用反射技术做动态赋值，所以属性名需要大小写严格匹配-->
  <object Name="refreshToolStripButton" Text="刷新" />
  
  <!--resource节点指定此窗体需要用的资源文件-->
  <resource id ="1" file="dialog.img.resources" key="img.340x60.dialog.top" />
  
  <!--text节点提供窗体里用name获取文本值-->
  <text name="ShowWords" value="在以下列表中添加字体" />
  
</resourceText>