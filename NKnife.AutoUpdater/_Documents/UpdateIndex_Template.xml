<?xml version="1.0" encoding="utf-8"?>
<!--面向平安力合自动更新逻辑的索引文件-->
<!--文件中所有版本号均为程序集的可用真实版本号-->
<!--属性：本文件的解析器的版本号-->
<Pansoft.Updater Version = "4">
  <!--更新器的描述节点-->
  <!--属性:是否有新的更新，版本号-->
  <Updater Version="2.1206">
    Pansoft.Updater.2.1206.zip
  </Updater>
  <Files>
    <!--增量版本，在两个版本之间的均使用该版本进行更新。如增量版本符合要求，将不考虑全量版本。-->
    <Incremental MinVersion="5.5.1204.5567" MaxVersion="5.5.1205.6146">
      CQ510.5.5.1205.6146.incre.zip
    </Incremental>
    <!--全量版本-->
    <Full Version="5.5.1205.6146">
      CQ510.5.5.1205.6146.full.zip
    </Full>
  </Files>
  <!--补丁程序，可多个，仅执行一次后被删除-->
  <Patchs>
    <!--Patch的执行文件(补丁程序)，用来如配置文件的升级等，仅更新后执行一次-->
    <Patch MinVersion="5.5.1204.5567" MaxVersion="5.5.1205.6146">PublicParamPatch.exe</Patch>
  </Patchs>
</Pansoft.Updater>