<?xml version="1.0" encoding="utf-8"?>

<MeterConfig>
  <Infomation><!--仪器的描述节点-->
    <Item title="brand"></Item>
    <Item title="model"></Item>
  </Infomation>
  <!--Commands：以下是仪器的指令列表描述节点-->
  <Commands isScpi="true" format="scpi">
    <command content="重置" command="*RST"></command>
    <command content="清屏" command="*CLS"></command>
    <command content="初始化" command="INIT"></command>
    <command content="输出" command="FETC" isReturn="true"></command>
    <command content="读取" command="READ" isReturn="true"></command>
    <command content="查询测量项目" command="CONF" isReturn="true"></command>
    <command content="配置测量项目并测量" command="MEAS" isReturn="true">
      <command content="直流电压" command="VOLT:DC"></command>
      <command content="交流电压" command="VOLT:AC"></command>
      <command content="直流电流" command="CURR:DC"></command>
      <command content="交流电流" command="CURR:AC"></command>
      <command content="电阻" command="RES"></command>
      <command content="四线电阻" command="FRES"></command>
      <command content="频率" command="FREQ"></command>
      <command content="周期" command="FER"></command>
      <command content="通断" command="CONT"></command>
      <command content="二极管" command="DIOD"></command>
    </command>
    <command content="测量项目" command="CONF" isConfig="true">
      <command content="直流电压" command="VOLT:DC">
        <group content="量程">
          <command content="直流电压" command="VOLT:DC:RANG">
            <param content=""></param>
            <param content="最大值" command="MAX"></param>
            <param content="最小值" command="MIN"></param>
          </command>
        </group>
      </command>
      <command content="交流电压" command="VOLT:AC">
        <group content="量程">
          <command content="交流电压" command="CURR:AC:RANG">
            <param content=""></param>
            <param content="最大值" command="MAX"></param>
            <param content="最小值" command="MIN"></param>
          </command>
        </group>
      </command>
      <command content="直流电流" command="CURR:DC">
        <group content="量程">
          <command content="直流电流" command="VOLT:DC:RANG">
            <param content=""></param>
            <param content="最大值" command="MAX"></param>
            <param content="最小值" command="MIN"></param>
          </command>
        </group>
      </command>
      <command content="交流电流" command="CURR:AC">
        <group content="量程">
          <command content="交流电流" command="CURR:AC:RANG">
            <param content=""></param>
            <param content="最大值" command="MAX"></param>
            <param content="最小值" command="MIN"></param>
          </command>
        </group>
      </command>
      <command content="电阻" command="RES">
        <group content="量程">
          <command content="电阻" command="RES:RANG">
            <param content=""></param>
            <param content="最大值" command="MAX"></param>
            <param content="最小值" command="MIN"></param>
          </command>
        </group>
      </command>
      <command content="四线电阻" command="FRES">
        <group content="量程">
          <command content="四线电阻" command="FRES:RANG">
            <param content=""></param>
            <param content="最大值" command="MAX"></param>
            <param content="最小值" command="MIN"></param>
          </command>
        </group>
      </command>
      <command content="频率" command="FREQ">
        <group content="量程">
          <command content="频率" command="FREQ:RANG">
            <param content=""></param>
            <param content="最大值" command="MAX"></param>
            <param content="最小值" command="MIN"></param>
          </command>
        </group>
      </command>
      <command content="周期" command="FER">
        <group content="量程">
          <command content="周期" command="PER:RANG">
            <param content=""></param>
            <param content="最大值" command="MAX"></param>
            <param content="最小值" command="MIN"></param>
          </command>
        </group>
      </command>
      <command content="通断" command="CONT">
        <group content="量程">
          <command content="通断" command="CONT:RANG">
            <param content=""></param>
            <param content="最大值" command="MAX"></param>
            <param content="最小值" command="MIN"></param>
          </command>
        </group>
      </command>
      <command content="二极管" command="DIOD">
        <group content="量程">
          <command content="二极管" command="DIOD:RANG">
            <param content=""></param>
            <param content="最大值" command="MAX"></param>
            <param content="最小值" command="MIN"></param>
          </command>
        </group>
      </command>
    </command>
    <command content="自动调零" command="ZERO:AUTO" isConfig="true">
      <param content="启动" command="ON"></param>
      <param content="停用" command="OFF"></param>
      <param content="仅一次" command="ONCE"></param>
    </command>
    <command content="输入高阻" command="INP:IMP:AUTO" isConfig="true">
      <param content="关闭" command="ON"></param>
      <param content="启用" command="OFF"></param>
    </command>
  </Commands>
</MeterConfig>